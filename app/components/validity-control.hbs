<legend>{{t 'validity'}}</legend>
<this.form.element @controlType="text" @label={{t "start_date"}} @placeholder="YYYY-MM-DD HH:mm" @disabled={{this.disabled}} @value={{this.startDate}} />

<this.form.element @controlType="text" @label={{t "end_date"}} @placeholder="YYYY-MM-DD HH:mm" @disabled={{this.disabled}} @value={{this.endDate}} />

<this.form.element @label={{t "enabled"}} @disabled={{this.disabled}} @controlType="checkbox" @property={{this.enabledProperty}} />