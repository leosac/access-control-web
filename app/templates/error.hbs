<div class="row-centered">
    <h1>Oh no, something went wrong.</h1>

    <div class="col-centered panel panel-default">
        <div class="panel-body">
          {{outlet}}

          <hr>
            <div id="content">
                <h4>Status Code: {{model.status_code}}</h4>
                <h4>Message: {{model.status_string}}</h4>
                <h4>Request Identifier: {{model.uuid}}</h4>
            </div>
        </div>
    </div>
</div>
