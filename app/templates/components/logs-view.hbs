<table class="table table-hover">
    <thead>
    <tr>
        <th>Id</th>
        <th>{{t 'message'}}</th>
        <th>{{t 'date'}}</th>
    </tr>
    </thead>
    <tbody>
    {{#each logs as |log|}}
        <tr>
            <td>{{log.id}}</td>
            <td>{{log.description}}</td>
            <td>{{log.timestamp}}</td>
        </tr>
    {{/each}}
    </tbody>
</table>
