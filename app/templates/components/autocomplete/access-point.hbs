<this.form.element @label={{this.label}}>
  <PowerSelect @searchEnabled={{true}} @search={{action "searchAP"}} @selected={{this.selected}} @placeholder={{t "autocomplete.search_access_point"}} @onChange={{action "changed"}} as |accessPointInfo|>
    {{accessPointInfo.alias}}
  </PowerSelect>
  <a href="#" {{action 'clear'}}>{{t 'clear_selection'}}</a>
</this.form.element>
