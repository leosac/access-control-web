{{#form.element label=this.label}}
    {{#power-select-typeahead search=(action 'searchGroup')
                              selected=this.selected
                              placeholder=(t 'autocomplete.search_group')
                              onChange=(action 'changed')
    as |accessPointInfo|}}
        {{accessPointInfo.alias}}
    {{/power-select-typeahead}}
    <a href="#" {{action 'clear'}}>{{t 'clear_selection'}}</a>
{{/form.element}}
