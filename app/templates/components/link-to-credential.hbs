{{#if this.credential.isRFIDCard}}
    <LinkTo @route='credentials.rfid-card' @model={{this.credential.id}}>
      <tr>
        {{this.text}}
      </tr>
    </LinkTo>
{{/if}}
{{#if this.credential.isPinCode}}
    <LinkTo @route='credentials.pin-code' @model={{this.credential.id}}>
      <tr>
        {{this.text}}
      </tr>
    </LinkTo>
{{/if}}
