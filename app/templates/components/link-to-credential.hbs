{{#if credential.isRFIDCard}}
    <LinkTo @route='credentials.rfid-card' @query={{hash id=credential.id}}>
      <tr>
        {{text}}
      </tr>
    </LinkTo>
{{/if}}
{{#if credential.isPinCode}}
    <LinkTo @route='credentials.pin-code' @query={{hash id=credential.id}}>
      <tr>
        {{text}}
      </tr>
    </LinkTo>
{{/if}}
