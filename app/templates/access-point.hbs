<div>
    <div class="col-md-12">
        <AccessPointForm @ap={{this.model}} @action={{route-action "editAP"}} @delete={{route-action "deleteAP"}} />
    </div>
</div>

{{#if model.door}}
    {{t 'accesspoint.belongs_to' dooralias=this.model.door.alias}}
{{else}}
    {{t 'accesspoint.no_belongs'}}
{{/if}}
